<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IWantYourNamJai"
    tools:layout_editor_absoluteY="81dp">

    <ImageView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/imageProfile"
        android:layout_width="80dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:layout_marginEnd="116dp"

        android:layout_marginStart="116dp"
        android:layout_marginTop="24dp"
        app:layout_constraintBottom_toTopOf="@+id/getName"
        app:layout_constraintEnd_toEndOf="@+id/buttonIwant"
        app:layout_constraintStart_toEndOf="@+id/IwantYear"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/dogsom" />

    <TextView
        android:id="@+id/IwantName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="120dp"
        android:text="Name"
        android:textColor="@color/darkPink"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/IwantNickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="Nickname"
        android:textColor="@color/darkPink"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IwantName" />

    <TextView
        android:id="@+id/IwantFaculty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="Faculty"
        android:textColor="@color/darkPink"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IwantNickname" />

    <TextView
        android:id="@+id/IwantYear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="Year"
        android:textColor="@color/darkPink"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IwantFaculty" />

    <TextView
        android:id="@+id/IwantLocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="Location"
        android:textColor="@color/darkPink"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IwantYear" />

    <TextView
        android:id="@+id/IwantTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="Time"
        android:textColor="@color/darkPink"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IwantDay" />

    <TextView
        android:id="@+id/IwantPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="Price"
        android:textColor="@color/darkPink"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IwantTime" />

    <TextView
        android:id="@+id/IwantContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="Contact"
        android:textColor="@color/darkPink"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IwantPrice" />

    <TextView
        android:id="@+id/getName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="53dp"
        android:layout_marginStart="100dp"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/IwantName"
        app:layout_constraintEnd_toEndOf="@+id/imageProfile"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/getNickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/getFaculty"
        app:layout_constraintStart_toStartOf="@+id/getName" />

    <TextView
        android:id="@+id/getFaculty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/IwantFaculty"
        app:layout_constraintStart_toStartOf="@+id/getNickname" />

    <TextView
        android:id="@+id/getYear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/IwantYear"
        app:layout_constraintStart_toStartOf="@+id/getFaculty" />

    <TextView
        android:id="@+id/getLocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/IwantLocation"
        app:layout_constraintStart_toStartOf="@+id/getYear" />

    <TextView
        android:id="@+id/getTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/IwantTime"
        app:layout_constraintStart_toStartOf="@+id/getPrice" />

    <TextView
        android:id="@+id/getPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/IwantPrice"
        app:layout_constraintStart_toStartOf="@+id/getContact" />

    <TextView
        android:id="@+id/getContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/IwantContact"
        app:layout_constraintEnd_toEndOf="@+id/getDay"
        app:layout_constraintStart_toStartOf="@+id/getDay" />

    <TextView
        android:id="@+id/IwantDay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="Day"
        android:textColor="@color/darkPink"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IwantLocation" />

    <TextView
        android:id="@+id/getDay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/IwantDay"
        app:layout_constraintStart_toStartOf="@+id/getLocation" />

    <Button
        android:id="@+id/buttonIwant"
        android:layout_width="0dp"
        android:layout_height="37dp"

        android:layout_marginEnd="16dp"

        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/buttonshape"
        android:fontFamily="@font/gillsans"
        android:gravity="center"
        android:onClick="onClick"
        android:shadowRadius="5"
        android:text="Iwantyournamjai"
        android:textColor="@color/cream"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IwantContact" />

</android.support.constraint.ConstraintLayout>